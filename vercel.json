
{
  "functions": {
    "server/index.ts": {
      "runtime": "@vercel/node@3.0.7"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/server/index.ts"
    },
    {
      "source": "/(.*)",
      "destination": "/client/dist/index.html"
    }
  ],
  "buildCommand": "cd client && npm install && npm run build"
}
